{
  "name": "collob-draw",
  "version": "0.3.22",
  "repository": {
    "type": "git",
    "url": "https://github.com/swarmjs/swarm-example.git"
  },
  "description": "Collaborative drawing application based on the Swarm",
  "main": "server.js",
  "scripts": {
    "build": "browserify -d client/app.js -o dist/build.js",
    "watch": "watchify -d client/app.js -o dist/build.js -v",
    "prestart": "make dist",
    "start": "node server.js --models=mice/model:conf/model --port:8000"
  },
  "keywords": [
    "swarm",
    "example"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.2.0",
    "express": "^4.9.7",
    "hammerjs": "^2.0.4",
    "leveldown": "^0.10.4",
    "minimist": "^1.1.0",
    "rx": "^2.5.2",
    "swarm": "^0.3.25",
    "swarm-restapi": "^0.3.24",
    "ws": "^0.4.32"
  },
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "browserify-shim": "^3.8.0",
    "watchify": "^2.4.0",
    "envify": "^3.0.0"
  }
}
